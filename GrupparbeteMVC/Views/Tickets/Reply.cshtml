@model GrupparbeteMVC.Models.Ticket


<h1>Information about the ticket</h1>



Title: @Html.DisplayFor(model => model.Title)
<br />

Text: @Html.DisplayFor(model => model.Description)
<br />

Author: @Html.DisplayFor(model => model.Creator)

<br />
<br />

Reply from Owner: @Html.DisplayFor(model => model.Reply)
<br />
<br />




<h1>Reply to Ticket</h1>

<form method="post">
    <div>
        <label for="reply">Reply:</label>
        <textarea id="reply" name="reply"></textarea>
    </div>
    <button type="submit">Submit</button>
</form>

